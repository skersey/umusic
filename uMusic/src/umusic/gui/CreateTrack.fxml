<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="createTrack" prefHeight="400.0" prefWidth="600.0" fx:controller="umusic.gui.CreateTrackController">
    <HBox>
        <children>
            <Label text="Track Name" />
            <TextField text="Track Name Value" fx:id="name" />
        </children>
    </HBox>
    <HBox>
        <children>
            <Label text="Track Type" />
            <fx:include source="choicebox/TrackTypes.fxml" fx:id="type"/>
        </children>
    </HBox>
    <HBox>
        <children>
            <Label text="Instrument" />
            <fx:include source="choicebox/Instruments.fxml" fx:id="instrument"/>
        </children>
    </HBox>
    <HBox>
        <Button text="Cancel" fx:id="closeButton" onAction="#closeButtonAction"/>
        <Button text="Create" fx:id="createButton" onAction="#createButtonAction"/>
    </HBox>
</VBox>